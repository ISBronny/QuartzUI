    "@angular/cli": "^17.0.9",
    "@angular/compiler-cli": "^17.0.8",
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "openapi-typescript-codegen": "^0.25.0",
    "typescript": "~5.2.2"
  }
