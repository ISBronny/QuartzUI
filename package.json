{
  "name": "quartz-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start prod": "ng serve --configuration production",
    "start dev": "ng serve --configuration development",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "api-client": "openapi --input ./src/OpenAPI/swagger.yaml --output ./src/dotnet-client/ --client angular",
    "generate-client": "openapi-generator-cli generate -i ./src/OpenAPI/swagger.yaml -g typescript-angular -o ./src/dotnet-client/ --additional-properties fileNaming=kebab-case,withInterfaces=true --generate-alias-as-model"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.8",
    "@angular/cdk": "^16.0.0",
    "@angular/common": "^17.0.8",
    "@angular/compiler": "^17.0.8",
    "@angular/core": "^17.0.8",
    "@angular/forms": "^17.0.8",
    "@angular/platform-browser": "^17.0.8",
    "@angular/platform-browser-dynamic": "^17.0.8",
    "@angular/router": "^17.0.8",
    "@taiga-ui/addon-charts": "^3.61.0",
    "@taiga-ui/addon-table": "^3.61.0",
    "@taiga-ui/addon-tablebars": "^3.61.0",
    "@taiga-ui/cdk": "^3.61.0",
    "@taiga-ui/core": "^3.61.0",
    "@taiga-ui/icons": "^3.61.0",
    "@taiga-ui/kit": "^3.61.0",
    "@taiga-ui/layout": "^3.61.0",
    "@taiga-ui/styles": "^3.61.0",
